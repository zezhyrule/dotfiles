#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

inline void keep_window_open() { char ch; cin>>ch; }

int main()	//C++ programs start by executing the function main
{
	cout << "Here we go!\n"; //output "Hello, World!"
	keep_window_open();		   //wait for a character to be entered
	return 0;
}
